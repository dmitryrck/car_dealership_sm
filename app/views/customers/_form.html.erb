<div class='row'>
  <div class='col-lg-12'>
    <%= simple_form_for @customer do |f| %>
      <%= f.input :full_name %>
      <%= f.input :cpf %>
      <%= f.input :birth_date, as: :string %>
      <%= f.input :rg %>
      <%= f.input :rg_sender %>
      <%= f.input :rg_expedition, as: :string %>
      <%= f.input :place_birth %>
      <%= f.input :place_birth_uf %>
      <%= f.input :nacionality %>
      <%= f.input :mother %>
      <%= f.input :father %>
      <%= f.input :postal_code %>
      <%= f.input :address %>
      <%= f.input :neighborhood %>
      <%= f.input :adjunct %>
      <%= f.input :city %>
      <%= f.input :state %>
      <%= f.input :phone %>
      <%= f.input :cell_phone %>
      <%= f.input :email %>

      <%= f.button :submit %>
    <% end %>
  </div><!-- /.col-lg-12 -->
</div><!-- /.row -->
