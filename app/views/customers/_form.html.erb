<%= simple_form_for @customer do |f| %>
  <%= f.input :full_name %>
  <%= f.input :cpf %>
  <%= f.input :birth_date, as: :text %>
  <%= f.input :rg %>
  <%= f.input :rg_sender %>
  <%= f.input :rg_expedition, as: :text %>
  <%= f.input :place_birth %>
  <%= f.input :place_birth_uf %>
  <%= f.input :nacionality %>
  <%= f.input :mother %>
  <%= f.input :father %>
  <%= f.input :postal_code %>
  <%= f.input :address %>
  <%= f.input :neighborhood %>
  <%= f.input :adjunct %>
  <%= f.input :city %>
  <%= f.input :state %>
  <%= f.input :phone %>
  <%= f.input :cell_phone %>
  <%= f.input :email %>

  <%= f.submit %>
<% end %>
