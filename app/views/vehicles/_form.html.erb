<%= simple_form_for @vehicle do |f| %>
  <%= f.error_notification %>

  <%= f.input :brand %>
  <%= f.input :version %>
  <%= f.input :maker_year %>
  <%= f.input :model_year %>
  <%= f.input :license_plate %>
  <%= f.input :city %>
  <%= f.input :uf %>
  <%= f.input :color %>
  <%= f.input :fuel %>
  <%= f.input :renavam %>
  <%= f.input :chassi %>
  <%= f.input :owner %>
  <%= f.input :old_license_plate %>
  <%= f.input :status %>

  <%= f.submit %>
<% end %>
